{
    "name": "hannesvdvreken/oauth",
    "type": "library",
    "description": "Laravel OAuth IoC wrapper Library",
    "keywords": ["OAuth", "Lusitanian", "laravel", "php", "wrapper", "config"],
    "homepage": "https://github.com/hannesvdvreken/laravel-oauth",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Van De Vreken",
            "homepage": "https://hannesvdvreken.com",
            "email": "vandevreken.hannes@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "lusitanian/oauth": "*"
    },
    "require-dev": {
        "illuminate/support": "4.1.*",
        "illuminate/session": "4.1.*",
        "mockery/mockery": "0.8.*"
    },
    "autoload": {
        "psr-0": {
            "hannesvdvreken\\OAuth\\": "src/"
        }
    },
    "suggest": {
        "hannesvdvreken/php-oauth": "A OAuth library built on top of the GuzzlePHP HTTP client"
    },
    "minimum-stability" : "dev"
